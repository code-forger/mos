	$(MAKE) -C joeprogram

.PHONY joeprogram2-code:
	$(MAKE) -C joeprogram2

.PHONY joeprogram3-code:
	$(MAKE) -C joeprogram3

.PHONY input_test-code:
	$(MAKE) -C input_test

.PHONY input_test2-code:
	$(MAKE) -C input_test2

.PHONY launcher-code:
